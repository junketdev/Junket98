<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.test.junket">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".QuestionnaireActivity"></activity>
        <activity android:name=".HotelListActivity" />
        <activity
            android:name=".HotelSearchActivity"
            android:theme="@style/AppTheme.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SplashscreenActivity"
            android:theme="@style/AppTheme.Fullscreen" />
        <activity
            android:name=".HotelviewActivity"
            android:theme="@style/AppTheme.Fullscreen" />
        <activity android:name=".Create_new_password" />
        <activity android:name=".Reset_password" />
        <activity
            android:name=".Forgot_password"
            android:theme="@style/AppTheme.Fullscreen" />
        <activity
            android:name=".LoginActivity"
            android:theme="@style/AppTheme.Fullscreen" />
        <activity
            android:name=".SignupActivity"
            android:theme="@style/AppTheme.Fullscreen" />
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>